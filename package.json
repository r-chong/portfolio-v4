{
    "name": "portfolio-v9",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "preflight": "node scripts/preflight.js",
        "preflight:ps": "powershell -ExecutionPolicy Bypass -File scripts/preflight.ps1",
        "preflight:quick": "npm run type-check && npm run lint",
        "preflight:fix": "npm run type-check && npm run lint:fix && npm run build",
        "test": "playwright test",
        "test:ui": "playwright test --ui",
        "test:debug": "playwright test --debug",
        "test:chromium": "playwright test --project=chromium",
        "test:firefox": "playwright test --project=firefox",
        "test:webkit": "playwright test --project=webkit",
        "test:mobile": "playwright test --project=\"Mobile Chrome\" --project=\"Mobile Safari\""
    },
    "overrides": {
        "next-contentlayer": {
            "next": "$next"
        }
    },
    "dependencies": {
        "@emotion/is-prop-valid": "^1.3.1",
        "@mdx-js/loader": "^3.0.1",
        "@mdx-js/react": "^3.0.1",
        "@next/mdx": "^14.1.3",
        "@next/third-parties": "^14.0.5-canary.59",
        "@types/mdx": "^2.0.11",
        "bundle-analyzer": "^0.0.6",
        "date-fns": "^3.6.0",
        "embla-carousel-react": "^8.5.2",
        "framer-motion": "^11.0.14",
        "gray-matter": "^4.0.3",
        "katex": "^0.16.21",
        "next": "14.1.3",
        "next-mdx-remote": "^4.4.1",
        "pdfjs-dist": "^4.10.38",
        "pliny": "^0.2.2",
        "react": "^18",
        "react-dom": "^18",
        "react-icons": "^5.0.1",
        "react-pdf": "^9.2.1",
        "react-toast": "^1.0.3",
        "react-vertical-timeline-component": "^3.6.0",
        "reading-time": "^1.5.0",
        "rehype-autolink-headings": "^7.1.0",
        "rehype-katex": "^6.0.3",
        "rehype-slug": "^6.0.0",
        "remark-breaks": "^4.0.0",
        "remark-gfm": "^4.0.1",
        "remark-math": "^5.1.1",
        "sharp": "^0.33.2"
    },
    "devDependencies": {
        "@playwright/test": "^1.51.0",
        "@tailwindcss/typography": "^0.5.10",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.1.3",
        "postcss": "^8",
        "tailwindcss": "^3.3.0",
        "typescript": "^5.3.2"
    }
}
